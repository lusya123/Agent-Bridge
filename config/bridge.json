{
  "machine_id": "cloud-a",
  "port": 9100,
  "capabilities": ["openclaw", "claude-code"],
  "max_agents": 5,
  "persistent_agents": [
    {
      "id": "ceo",
      "type": "openclaw",
      "auto_start": true,
      "workspace": "/home/agent/workspace-ceo"
    }
  ],
  "adapters": {
    "openclaw": {
      "gateway": "ws://127.0.0.1:18789"
    },
    "claude_code": {
      "tmux_session": "agents"
    }
  }
}
